/**index.wxss**/
.header{
  box-sizing:border-box;
  padding:10rpx 30rpx;
  border-radius:5px;
}
.sw-img{
  width:100%;
  height:150px;
  border-radius:5px;
}
.content{
  position:relative;
  margin:15px 15px 10px;
  padding:5px;
  box-sizing: border-box;
  background-color:#f6f6f6;
  border-radius:5px;
  box-shadow:0 0 3px rgba(0,0,0,.7);
}
.content .img{
  width:100%; 
  height:150px;
  border-radius:5px;
  cursor:pointer;
}
